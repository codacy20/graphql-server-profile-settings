// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgres"
  url      = "postgres://root:example@localhost:5432/mydb"
}

generator client {
  provider = "prisma-client-js"
}

model Profile {
  userName  String   @id @map("username")
  firstName String?
  lastName  String?
  location  String?
  bio       String?
  links     String[]
}
